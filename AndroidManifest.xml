<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.android.helloworld">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MessageActivity">
        </activity>
        <activity android:name=".RequestListActivity">
        </activity>
        <activity android:name=".EventViewActivity">
        </activity>
        <activity android:name=".EventInfoActivity">
        </activity>
        <activity android:name=".AddEventActivity">
        </activity>
        <activity android:name=".MenuActivity">
        </activity>
        <activity android:name=".WriteReportActivity">
        </activity>
        <activity android:name=".WriteReviewActivity">
        </activity>
        <activity android:name=".HostInfoActivity">
        </activity>
        <activity android:name=".HostsViewActivity">
        </activity>
        <activity android:name=".ProfileViewActivity">
        </activity>
        <activity android:name=".AddProfileActivity">
        </activity>
        <activity android:name=".SignUpActivity">
        </activity>
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>